{
  "name": "git-needs-push",
  "version": "1.0.0",
  "description": "Check synchronously if a git repository needs to push",
  "main": "build.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "nyc ava",
    "lint": "eslint index.js",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "prepublish": "npm run babel",
    "babel": "babel index.js -o build.js",
    "prepush": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JPeer264/node-git-needs-push.git"
  },
  "keywords": [
    "is",
    "git",
    "repository",
    "repo",
    "is-git",
    "needs",
    "push"
  ],
  "author": "Jan Peer St√∂cklmair",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JPeer264/node-git-needs-push/issues"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "homepage": "https://github.com/JPeer264/node-git-needs-push#readme",
  "dependencies": {
    "current-git-branch": "^1.0.0",
    "execa": "^0.6.1",
    "is-git-repository": "^1.1.1",
    "path-is-absolute": "^1.0.1"
  },
  "devDependencies": {
    "@alrra/travis-scripts": "^3.0.1",
    "ava": "^0.19.1",
    "babel-cli": "^6.24.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.2.2",
    "coveralls": "^2.12.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb-base": "^11.1.1",
    "eslint-plugin-import": "^2.2.0",
    "fs-extra": "^3.0.1",
    "husky": "^0.13.2",
    "nyc": "^10.1.2"
  }
}
